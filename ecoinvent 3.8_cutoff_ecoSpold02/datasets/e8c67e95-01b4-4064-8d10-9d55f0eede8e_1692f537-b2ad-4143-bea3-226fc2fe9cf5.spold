      <intermediateExchange id="30ed3e94-340e-47c9-a1bd-7861766962ad" unitId="487df68b-4994-4027-8fdc-a4dc298257b7" variableName="glass_plastic" amount="14194.62" isCalculatedAmount="true" mathematicalRelation="21507*0.6*1.1" sourceId="b93a3031-a4fd-497f-9a90-274db9a72eae" sourceYear="2004" sourceFirstAuthor="Elsam Engineering A/S,Fredericia,Denmark" pageNumbers="p. 25, Tab. 5.3" intermediateExchangeId="0798a5d1-6713-4c82-8ec9-c40ac2548279" activityLinkId="5f2b9a49-6455-4f2e-afdf-84af3900830f">
