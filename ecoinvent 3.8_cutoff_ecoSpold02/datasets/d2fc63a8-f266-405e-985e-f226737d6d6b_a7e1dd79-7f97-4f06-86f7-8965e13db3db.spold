      <parameter parameterId="3dce430c-9c14-453e-b90b-352fdecc8043" variableName="CH4_natural" mathematicalRelation="CH4rate_natural*total_occupation_period/yield_per_ha" isCalculatedAmount="true" amount="0.749372351173188">
