      <parameter parameterId="f7f287b0-1e10-4dd5-bb58-f2f51019e5f0" variableName="precipitation" mathematicalRelation="550+0.1* irrigation* yield_kg_ha/10" isCalculatedAmount="true" amount="677.5733333456">
