      <activity id="d9387377-b75a-4469-add4-43ac9e244c5a" activityNameId="ec30d7ec-20e2-5a58-85f7-3135dd733610" parentActivityId="a32fa52b-805c-5f3b-bcf4-fda7a8bb18dd" inheritanceDepth="-1" type="1" specialActivityType="0">
        <activityName xml:lang="en">azodicarbonamide production</activityName>
        <synonym xml:lang="en">ADC</synonym>
        <synonym xml:lang="en">1,1'-azobisformamide</synonym>
        <synonym xml:lang="en">azobiscarboxamide</synonym>
        <synonym xml:lang="en">azodicarboxamide</synonym>
        <synonym xml:lang="en">azodicarbodiamide</synonym>
