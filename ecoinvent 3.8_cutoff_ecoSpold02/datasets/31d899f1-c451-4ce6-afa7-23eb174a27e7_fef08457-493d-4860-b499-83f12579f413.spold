        <uncertainty>
          <lognormal meanValue="0.05" mu="-2.996" variance="0.0006" varianceWithPedigreeUncertainty="0.0107" />
          <pedigreeMatrix reliability="3" completeness="2" temporalCorrelation="1" geographicalCorrelation="1" furtherTechnologyCorrelation="3" />
