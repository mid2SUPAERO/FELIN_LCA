        <activityName xml:lang="en">ascorbic acid production</activityName>
        <synonym xml:lang="en">L(+)-Ascorbic acid</synonym>
        <synonym xml:lang="en">Ascorbic acid</synonym>
        <synonym xml:lang="en">L-ascorbic acid</synonym>
