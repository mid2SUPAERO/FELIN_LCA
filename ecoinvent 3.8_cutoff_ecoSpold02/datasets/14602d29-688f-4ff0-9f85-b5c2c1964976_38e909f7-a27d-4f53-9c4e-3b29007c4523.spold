      <intermediateExchange id="45aa44a2-fb6f-42ef-ba54-7c532f7fd9b9" unitId="487df68b-4994-4027-8fdc-a4dc298257b7" casNumber="007782-44-7" amount="0.0956217783515937" intermediateExchangeId="28c2473e-1e11-4078-9a76-de9550553adc" activityLinkId="e687c5b5-3ae7-4210-b683-da7bc0f62345">
