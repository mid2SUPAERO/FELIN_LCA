      <parameter parameterId="8a736c2d-b252-4002-b12b-329dc8481202" variableName="volume_floor" mathematicalRelation="((reefer_RP_length_external* reefer_RP_width_external)*(2/1000))+(35*((((63.5-4)*2)+(28.5*2))/1000000)* reefer_RP_length_external)" isCalculatedAmount="true" amount="0.134550912">
