      <intermediateExchange id="57994711-9d2e-460e-aca1-bb4edaee3a85" unitId="487df68b-4994-4027-8fdc-a4dc298257b7" variableName="RP_amount_1" amount="1" intermediateExchangeId="8f5d26e5-ea22-432f-93e5-362a6df5ec66" productionVolumeAmount="7800000" productionVolumeVariableName="RP_PV_1">
