          <pedigreeMatrix reliability="5" completeness="5" temporalCorrelation="5" geographicalCorrelation="1" furtherTechnologyCorrelation="5" />
          <comment xml:lang="en">Swiss data. Large deviation of literature data.</comment>
        </uncertainty>
