          <pedigreeMatrix reliability="1" completeness="3" temporalCorrelation="5" geographicalCorrelation="1" furtherTechnologyCorrelation="1" />
          <comment xml:lang="en">Environmental report, Placer Dome Inc. 2003</comment>
        </uncertainty>
