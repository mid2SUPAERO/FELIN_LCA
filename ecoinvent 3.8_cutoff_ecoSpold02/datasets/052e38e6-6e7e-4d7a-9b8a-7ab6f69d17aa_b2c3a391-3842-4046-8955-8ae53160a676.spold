  <childActivityDataset>
    <activityDescription>
      <activity id="052e38e6-6e7e-4d7a-9b8a-7ab6f69d17aa" activityNameId="2316f0a2-ef1a-43ea-9459-f280f1e421a0" parentActivityId="6147443a-49d4-45b7-af93-484d00aa9ffb" inheritanceDepth="-1" type="1" specialActivityType="0">
        <activityName xml:lang="en">cement production, pozzolana and fly ash 36-55%</activityName>
        <synonym xml:lang="en">Portland cement with additions</synonym>
        <synonym xml:lang="en">type IP</synonym>
        <includedActivitiesStart xml:lang="en">This activity starts with the reception of raw materials.</includedActivitiesStart>
        <includedActivitiesEnd xml:lang="en">This activity ends with the cement produced in the cement mill. Packaging is not included and no production waste is assumed. </includedActivitiesEnd>
        <generalComment>
          <text xml:lang="en" index="1">The dataset describes the production of cement type IP according to NTP 334.009 / ASTM C150  for Portland cements.</text>
          <text xml:lang="en" index="2">The weight composition of the cement is: 60% clinker, 2.4% gypsum and 37.6% pozzolana.</text>
          <text xml:lang="en" index="4">The cement density is 3g/ml and the resitance is 35MPa according to the NTP 334.090 standard (product description of cement IP of UNACEM).</text>
        </generalComment>
      </activity>
      <classification classificationId="36cd730c-4485-4fe9-8efe-8850f771796d">
        <classificationSystem xml:lang="en">ISIC rev.4 ecoinvent</classificationSystem>
        <classificationValue xml:lang="en">2394:Manufacture of cement, lime and plaster</classificationValue>
      </classification>
      <geography geographyId="11387d6c-7d7e-11de-9ae2-0019e336be3a">
        <shortname xml:lang="en">PE</shortname>
        <comment>
          <text xml:lang="en" index="1">Representative production of cement type IP in Peru. Data based from main cement producers and personal comunications with local experts.</text>
        </comment>
      </geography>
      <technology technologyLevel="3">
        <comment>
          <text xml:lang="en" index="1">Represent the current technology for cement production in Peru.</text>
        </comment>
      </technology>
      <timePeriod startDate="2014-01-01" endDate="2021-12-31" isDataValidForEntirePeriod="false">
        <comment>
          <text xml:lang="en" index="1">The production volume of 2016 is considered. Data from sustainability reports of 2014 to 2017 is used.</text>
        </comment>
      </timePeriod>
      <macroEconomicScenario macroEconomicScenarioId="d9f57f0a-a01f-42eb-a57b-8f18d6635801">
        <name xml:lang="en">Business-as-Usual</name>
      </macroEconomicScenario>
    </activityDescription>
