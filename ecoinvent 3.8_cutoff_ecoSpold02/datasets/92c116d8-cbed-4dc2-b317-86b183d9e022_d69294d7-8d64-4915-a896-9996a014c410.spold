      <parameter parameterId="fb56edfc-6599-4dac-b6f8-3bf2f194cb48" variableName="corr" mathematicalRelation="" amount="0.92">
        <name xml:lang="en">correction factor</name>
        <unitName xml:lang="en">dimensionless</unitName>
