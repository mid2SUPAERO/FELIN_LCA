          <lognormal meanValue="5.27426160337553E-08" mu="-16.76" variance="0.0006" varianceWithPedigreeUncertainty="0.2092" />
          <pedigreeMatrix reliability="5" completeness="5" temporalCorrelation="5" geographicalCorrelation="4" furtherTechnologyCorrelation="5" />
          <comment xml:lang="en"></comment>
        </uncertainty>
