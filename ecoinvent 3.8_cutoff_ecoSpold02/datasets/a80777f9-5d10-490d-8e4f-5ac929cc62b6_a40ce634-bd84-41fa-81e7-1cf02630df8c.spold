        <property propertyId="ff8ff2b1-54a1-4336-807e-89b2981eb47a" variableName="utilization_rate" amount="0.266" isDefiningValue="false" mathematicalRelation="0.38*0.7" isCalculatedAmount="true" unitId="577e242a-461f-44a7-922c-d8e1c3d2bf45" sourceId="93534758-4518-48b3-add2-f0c6770ecdae" sourceYear="2000" sourceFirstAuthor="Crinks, P.">
